{
  "configurations": {
    "python-adaptor": {
      "adapter": "debugpy",
      "configuration": {
        "type": "python",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "stopOnEntry": false,
        "console": "externalTerminal",
        "debugOptions": [],
        "program": "${file}"
      },
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "",
          "userUnhandled": ""
        }
      }
    }
  }
}
